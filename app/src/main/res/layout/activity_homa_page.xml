                android:layout_marginLeft="131dp"
                android:layout_marginTop="45dp"
                android:layout_width="123dp"
                android:layout_height="62dp"
                android:text="Welcome to Wishwash" />

            <TextView
                android:layout_gravity="center_horizontal|center"
