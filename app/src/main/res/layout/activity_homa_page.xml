                android:layout_marginTop="429dp"
                android:layout_width="207dp"
                android:layout_height="47dp"
                android:text="Wash per Kilogram" />
            <TextView
                android:textSize="14sp"
                android:textColor="@color/welcome_to_wishwash_color"
                android:layout_marginLeft="177dp"
                android:layout_marginTop="616dp"
                android:layout_width="207dp"
                android:layout_height="47dp"
                android:text="History"
                />

            <ImageView
                android:contentDescription="image_22"
                android:src="@drawable/_image_22"
                android:layout_marginStart="79dp"
                android:layout_marginTop="600dp"
                android:layout_width="64dp"
                android:layout_height="68dp"
                android:id="@+id/history_section"
                android:clickable="true"
                android:focusable="true"
                />

        </RelativeLayout>
    </LinearLayout>
</LinearLayout>
