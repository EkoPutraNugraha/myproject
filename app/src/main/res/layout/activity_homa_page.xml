                android:textColor="@color/welcome_to_wishwash_color"
                android:layout_marginLeft="151dp"
                android:layout_marginTop="179dp"
                android:layout_width="108dp"
                android:layout_height="39dp"
                android:text="Hi "
                android:id="@+id/user_name_text_view"/>

            <TextView
                android:textSize="13sp"
                android:textColor="@color/welcome_to_wishwash_color"
                android:layout_marginLeft="176dp"
                android:layout_marginTop="323dp"
                android:layout_width="163.5dp"
                android:layout_height="27.5dp"
                android:text="Estimation 2/3 days" />

            <TextView
                android:textSize="13sp"
                android:textColor="@color/welcome_to_wishwash_color"
                android:layout_marginLeft="177dp"
                android:layout_marginTop="480dp"
                android:layout_width="157.5dp"
                android:layout_height="34.5dp"
                android:text="Estimation 3/4 days" />

            <TextView
                android:textSize="13sp"
                android:textColor="@color/welcome_to_wishwash_color"
                android:layout_marginLeft="177dp"
                android:layout_marginTop="640dp"
                android:layout_width="157.5dp"
                android:layout_height="34.5dp"
                android:text="Your Order History"
                />

            <ImageView
                android:src="@drawable/image_14"
                android:contentDescription="image_14"
                android:layout_marginStart="41dp"
                android:layout_marginTop="53dp"
                android:layout_width="60dp"
                android:layout_height="60dp" />

            <View
                android:background="@drawable/_rectangle_24_ek1_shape"
                android:layout_marginStart="58dp"
                android:layout_marginTop="256dp"
                android:layout_width="98dp"
                android:layout_height="98dp" />

            <View
                android:background="@drawable/_rectangle_36_shape"
